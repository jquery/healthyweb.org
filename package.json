{
  "name": "healthyweb.org",
  "type": "module",
  "version": "0.1.0",
  "scripts": {
    "astro": "astro",
    "build": "npm run clean && astro check && astro build",
    "clean": "rm -rf dist .astro .wrangler",
    "deploy": "npm run build && wrangler pages deploy",
    "dev": "astro dev",
    "format": "eslint . --fix && prettier --write .",
    "lint": "concurrently \"eslint .\" \"prettier --check .\" \"npm run typescript\"",
    "preview": "npm run clean && astro check && astro build && wrangler pages dev",
    "start": "npm run dev",
    "test": "astro check && npm run lint",
    "types": "wrangler types",
    "typescript": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/cloudflare": "^12.6.12",
    "@astrojs/svelte": "^7.2.2",
    "@fontsource/roboto-slab": "^5.2.8",
    "astro": "^5.16.3",
    "svelte": "^5.45.2",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@cloudflare/puppeteer": "^1.0.4",
    "@cloudflare/workers-types": "^4.20251128.0",
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.17",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "prettier": "^3.7.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "typescript-eslint": "^8.48.0",
    "wrangler": "^4.51.0"
  }
}
