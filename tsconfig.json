{
  "extends": "astro/tsconfigs/strict",
  "exclude": ["node_modules", "dist"],
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "node16",
    "moduleResolution": "node16",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "types": ["@cloudflare/workers-types/2023-07-01"]
  }
}
